{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true
  },
  "projects": {
    "objective": {
      "type": "library",
      "root": "libs/objective",
      "entryFile": "index",
      "sourceRoot": "libs/objective/src",
      "compilerOptions": {
        "tsConfigPath": "libs/objective/tsconfig.lib.json"
      }
    },
    "recording": {
      "type": "library",
      "root": "libs/recording",
      "entryFile": "index",
      "sourceRoot": "libs/recording/src",
      "compilerOptions": {
        "tsConfigPath": "libs/recording/tsconfig.lib.json"
      }
    }
  }
}