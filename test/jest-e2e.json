{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "okr/objective/(.*)": "<rootDir>/../libs/objective/src/$1",
    "okr/objective": "<rootDir>/../libs/objective/src",
    "@okr/recording/(.*)": "<rootDir>/../libs/recording/src/$1",
    "@okr/recording": "<rootDir>/../libs/recording/src"
  }
}